{
  "manifestVersion": 1,
  "id": "team-calendar-2019",
  "publisher": "ms-devlabs",
  "version": "2.0.16",
  "name": "Team Calendar 2019",
  "description": "Track events important to your team, view and manage days off, quickly see when sprints start and end, and more. Support for Azure DevOps 2019 and earlier.",
  "public": true,
  "categories": ["Azure Boards"],
  "targets": [
    {
      "id": "Microsoft.TeamFoundation.Server",
      "version": "[10.0, 17.0]"
    }
  ],
  "scopes": ["vso.work_write"],
  "icons": {
    "default": "static/v2-images/calendar-logo.png"
  },
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE.txt"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/microsoft/vsts-team-calendar/issues"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vsts-team-calendar"
  },
  "screenshots": [
    {
      "path": "static/v2-images/calendar-screen-shot.png"
    }
  ],
  "files": [
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "legacy/static",
      "packagePath": "static",
      "addressable": true
    },
    {
      "path": "legacy/dist/js",
      "packagePath": "js",
      "addressable": true
    },
    {
      "path": "legacy/dist/css",
      "packagePath": "css",
      "addressable": true
    },
    {
      "path": "legacy/node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "sdk"
    },
    {
      "path": "legacy/node_modules/jquery/dist",
      "addressable": true,
      "packagePath": "lib/jquery"
    },
    {
      "path": "legacy/node_modules/moment/min",
      "addressable": true,
      "packagePath": "lib/moment"
    },
    {
      "path": "legacy/node_modules/fullcalendar/dist",
      "addressable": true,
      "packagePath": "lib/fullcalendar"
    }
  ]
}
